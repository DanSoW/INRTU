
class MaxMinObj {
  public float max_x;
  public float min_x;
  
  public float max_y;
  public float min_y;
  
  public float max_z;
  public float min_z;
  
  public MaxMinObj(float max_x, float min_x, float max_y, float min_y, float max_z, float min_z){
    this.max_x = max_x;
    this.min_x = min_x;
    
    this.max_y = max_y;
    this.min_y = min_y;
    
    this.max_z = max_z;
    this.min_z = min_z;
  }
}
